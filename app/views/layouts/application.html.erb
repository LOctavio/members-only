<!DOCTYPE html>
<html>
  <head>
    <title>MembersOnly</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <% if user_signed_in? %>
      <p> <%= link_to  current_user.name, edit_user_registration_path %> </p>
      <p> <%= link_to  "Log out", destroy_user_session_path, method: :delete %> </p>
    <% else%>
      <p> <%= link_to  "Sign in", new_user_session_path %> </p>
      <p> <%= link_to  "Sign up", new_user_registration_path %> </p>
    <% end %>
    <%= yield %>
  </body>
</html>
